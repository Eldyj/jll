(let factorial
	(if (= 0 (% 0))
			1
			(* (% 0)
				 (factorial (- (% 0) 1)))))

(let triangular
	(if (= 0 (% 0))
			0
			(+ (% 0)
				 (triangular (- (% 0) 1)))))

(let fibonnacci
	(if (| (= 0 (% 0))
				 (= 1 (% 0)))
			(% 0)
			(+ (fibonnacci (- (% 0) 1))
				 (fibonnacci (- (% 0) 2)))))

(let pow
	(if (= 0 (% 0)
			0
			(if (= 0 (% 1))
					(% 1)
					(pow (% 0) (- (% 1) 1))))))
